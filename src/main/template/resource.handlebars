<div class='heading'>
  <h2>
    <a href='#!/{{name}}' onclick="Docs.toggleEndpointListForResource('{{name}}');">{{name}} {{#description}} : {{/description}}{{{description}}}</a>
  </h2>
  <ul class='options'>
    <li>
      <a href='#' onclick="Docs.toggleOperationsForResource('{{name}}'); return false;">
        List/Expand Operations
      </a>
    </li>
    <li>
      <a href='{{url}}' target="_blank">Resource Metadata</a>
    </li>
  </ul>
</div>
<ul class='endpoints' id='{{name}}_endpoint_list' style='display:none'>

</ul>
